the active TH element\n     */\n    mousemoveCall: function() {\n      document.addEventListener('mousemove', function(e) {\n        if (typeof resizeTable.def.thActive === 'undefined' || !resizeTable.def.thActive) {\n          return;\n        }\n        resizeTable.def.thActive.style.width = resizeTable.def.offset + e.pageX + 'px';\n      });\n    },\n\n    /**\n     * @function\n     * @description: main initialization\n     */\n    init: function() {\n      for (var i = 0, len = resizeTable.def.thElmnts.length; i < len; i++) {\n        var iThEle = resizeTable.def.thElmnts[i];\n\n        resizeTable.mouseenterCall(iThEle);\n        resizeTable.mouseleaveCall(iThEle);\n        resizeTable.mousedownCall(iThEle);\n      }\n\n      resizeTable.mouseupCall();\n      resizeTable.mousemoveCall();\n    }\n  };\n\n  resizeTable.init();\n\n})();"]],"language":[["JavaScript"]]},"created_time":1690464768847,"last_edited_time":1690464779135,"parent_id":"b405aa57-699b-4a21-b51a-848d5fff95c2","parent_table":"block","alive":true,"created_by_table":"notion_user","created_by_id":"ddedc31a-d3a4-4262-a9fb-9d1fb5daf959","last_edited_by_table":"notion_user","last_edited_by_id":"ddedc31a-d3a4-4262-a9fb-9d1fb5daf959","space_id":"9670d216-af32-4346-b6e8-3286d08ab53c"},"role":"comment_only"},"321c4d7f-d5ca-4b9b-be8c-1db44137973c":{"value":{"id":"321c4d7f-d5ca-4b9b-be8c-1db44137973c","version":18,"type":"code","properties":{"title":[["table,\nth,\ntd {\n  border: 1px solid #000;\n}\n\ntable {\n  border-collapse: collapse;\n}\n\nth,\ntd {\n  padding: 5px;\n}\n\nth {\n  position: relative;\n}\n\nth span {\n  position: absolute;\n  background-color: #FF0000;\n  width: 5px;\n  right: -2px;\n  top: 0;\n  bottom: 0;\n  left: auto;\n  cursor: col-resize;\n}"]],"language":[["HTML"]]},"created_time":1690464781255,"last_edited_time":1690464788275,"parent_id":"b405aa57-699b-4a21-b51a-848d5fff95c2","parent_table":"block","alive":true,"created_by_table":"notion_user","created_by_id":"ddedc31a-d3a4-426